{
  "compilerOptions": {
    "module": "es2015",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist/",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
